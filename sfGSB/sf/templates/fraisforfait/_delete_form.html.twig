<form method="post" action="{{ path('fraisforfait_delete', {'id': fraisforfait.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce frais ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ fraisforfait.id) }}">
    <br/>
    <button class="btn-menu animated fadeInUp scrollto" style="background-color: red;">Supprimer</button>           
   
</form>
